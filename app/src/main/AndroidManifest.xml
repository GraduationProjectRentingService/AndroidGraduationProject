<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sunxuedian.graduationproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.HouseDetailActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.BrowserActivity" />
        <activity android:name=".view.activity.HouseListSwipeBackActivity" />
        <activity android:name=".view.activity.LoginSwipeBackActivity" />
        <activity android:name=".view.activity.RegisterSwipeBackActivity" />
        <activity android:name=".view.activity.MainActivity"></activity>
    </application>

</manifest>