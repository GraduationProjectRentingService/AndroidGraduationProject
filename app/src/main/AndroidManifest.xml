<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sunxuedian.graduationproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.BrowserActivity" />
        <activity android:name=".view.activity.HouseListActivity" />
        <activity android:name=".view.activity.LoginActivity" />
        <activity android:name=".view.activity.RegisterActivity" />
        <activity android:name=".view.activity.HouseDetailActivity" />
        <activity android:name=".view.activity.ReverseHouseActivity" />
        <activity android:name=".view.activity.ChooseCheckInPeopleActivity" />
        <activity android:name=".view.activity.AddCheckInPeopleActivity" />
        <activity android:name=".view.activity.PayOrderActivity" />
        <activity android:name=".view.activity.OrderDetailActivity" />
        <activity android:name=".view.activity.UserInfoActivity" />
        <activity android:name=".view.activity.SetUserNameActivity" />
        <activity android:name=".view.activity.OrderListActivity" />
        <activity android:name=".view.activity.LikeHouseListActivity"></activity>
    </application>

</manifest>